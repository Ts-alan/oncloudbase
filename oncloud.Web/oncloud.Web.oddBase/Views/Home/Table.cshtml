@model IEnumerable<oncloud.Web.oddBase.Models.Home.Table>

@{
    ViewBag.Title = "Table";
}

@Scripts.Render("~/bundles/jqCrid")
@Styles.Render("~/Content/jqCrid")
<script type="text/javascript">
    $(document).ready(function() {
        tableToGrid("#table", {
            height: 700,
            colNames: [
                "№ улицы", "Населенный пункт", "Наименование",
                "Количество участков", "     "
            ],
            forceFit: false,
            cmTemplate: { "resizable": false, sortable: false },
            colModel: [
                { name: "№_улицы", align: "center" },
                { name: "Населенный_пункт", align: "center" },
                { name: "Наименование", align: "center" },
                { name: "Количество_участков", align: "center" },
                { name: "Столбце_действия", align: "center" }

            ]
        });
    });
</script>
@Html.ActionLink("+ Добавить новую рекламную конструкцию", "Index", null, new { @class = "btn btn-default" })
<table id="table" class="table-bordered" cellpadding="5" style="width: 100%">
    <tr>

        <th>№_улицы</th>
        <th>Населенный_пункт</th>
        <th> Наименование </th>
        <th> Количество_участков</th>
        <th>Столбце_действия</th>
    </tr>
    <tr>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
    </tr>
</table>






